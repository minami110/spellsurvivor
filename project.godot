config_version=5

[application]

config/name="fms"
config/version="0.4.0"
run/main_scene="res://Title/Title.tscn"
config/features=PackedStringArray("4.3", "C#", "GL Compatibility")
run/max_fps=60
config/icon="res://Common/Resources/Textures/icon.svg"

[autoload]

